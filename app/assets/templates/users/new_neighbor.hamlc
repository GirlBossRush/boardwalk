.field.writable
  %label New Neighbor
  %a.delete-neighbor{href: '#delete-neighbor'} &#x2715;
  %input.user-new-neighbor{type: 'text',
                           placeholder: 'Search for neighbor by username',
                           list: 'user-search',
                           name: 'user[neighbors][]',
                           optional: true,
                           autocomplete: 'off',
                           pattern: "^[a-zA-Z0-9\-_]+$"}

